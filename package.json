{
  "name": "leadforensics_launchpad",
  "version": "1.1.2",
  "author": {
    "name": "Lead Forensics",
    "email": "support@leadforensics.com",
    "url": "https://www.leadforensics.com"
  },
  "main": "./init.js",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently -k npm:dev-*",
    "dev-electron": "electron electron.js",
    "dev-vue": "vue-cli-service serve",
    "pack": "electron-builder --dir",
    "build-local-win": "vue-cli-service build && electron-builder --win --config './electron.build.windows.local.json' --publish never",
    "build-dev-win": "vue-cli-service build && electron-builder --win --config './electron.build.windows.dev.json' --publish always",
    "build-live-win": "vue-cli-service build && electron-builder --win --config './electron.build.windows.live.json' --publish always",
    "build-local-linux": "vue-cli-service build && electron-builder --linux --config './electron.build.linux.local.json' --publish never",
    "build-dev-linux": "vue-cli-service build && electron-builder --linux --config './electron.build.linux.dev.json' --publish always",
    "build-live-linux": "vue-cli-service build && electron-builder --linux --config './electron.build.linux.live.json' --publish always",
    "build-local-mac": "vue-cli-service build && electron-builder --mac --config './electron.build.mac.local.json' --publish never",
    "build-dev-mac": "vue-cli-service build && electron-builder --mac --config './electron.build.mac.dev.json' --publish always",
    "build-live-mac": "vue-cli-service build && electron-builder --mac --config './electron.build.mac.live.json' --publish always",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "browser-window": "^0.4.0",
    "core-js": "^2.6.5",
    "electron-debug": "^3.0.1",
    "electron-packager": "^14.0.4",
    "electron-prebuilt-compile": "^4.0.0",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.1.2",
    "fs": "0.0.1-security",
    "material-design-icons-iconfont": "^5.0.1",
    "path": "^0.12.7",
    "remote": "^0.2.6",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuetify": "^2.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "electron-builder": "^21.2.0",
    "electron-prebuilt-compile": "^4.0.0",
    "electron-settings": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "path": "^0.12.7",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.6.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  }
}
